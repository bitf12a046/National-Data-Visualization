                    <div class="modal-body">
                        <!-- Project Details Go Here -->
                        <h2>Pakage Request</h2>
                        <p class="item-intro text-muted">Fill In The Form With Your Desired Requirement.</p>

                        <form class="form" method="post" action="/">
                            <div class="panel panel-info">
                                <div class="panel-heading">
                                    <h3 class="panel-title">Request Details</h3>
                                </div>
                                <div class="panel-body">

                                    <div class="form-group">
                                        <label for="heading">Title</label>
                                        <input class="form-control" data-val="true" data-val-length="Title must be between 3 and 30 characters!" data-val-length-max="50" data-val-length-min="3" data-val-required="Title is required" id="title" name="title" placeholder="eg. Hp Laptop i5" type="text" value="" />
                                        <span class="field-validation-valid" data-valmsg-for="title" data-valmsg-replace="true"></span>
                                    </div>
                                    <div>
                                        <label for="heading">Catagory</label>
                                        <select class="form-control" id="cat" name="catagory">
                                            <option>Real Estate</option>
                                            <option>Vehicle</option>
                                            <option>Computer</option>
                                            <option>Cell Phone</option>
                                        </select>
                                        <br />
                                        <span class="field-validation-valid" data-valmsg-for="cat" data-valmsg-replace="true"></span>
                                    </div>

                                    <div class="form-group">
                                        <label for="heading">Brand</label>
                                        <input class="form-control" data-val="true" data-val-length="Brand Name must be between 3 and 50 characters!" data-val-length-max="50" data-val-length-min="3" data-val-required="Brand is required" id="brand" name="brand" placeholder="Apple, Nokia, LG" type="text" value="" />

                                        <span class="field-validation-valid" data-valmsg-for="brand" data-valmsg-replace="true"></span>
                                    </div>

                                    <div class="form-group">
                                        <label for="status">Status</label>


                                        <select class="form-control" id="status" name="status">
                                            <option selected="selected">New</option>
                                            <option>Used</option>
                                        </select>

                                    </div>

                                    <div class="form-group">
